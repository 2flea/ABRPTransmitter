{
  "manifest_version": 2,
  "name": "messaging",
  "version": "1.0",
  "description": "Example messaging web extension.",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "nativeMessaging",
    "geckoViewAddons",
    "<all_urls>",
    "tabs",
    "activeTab",
    "*://abetterrouteplanner.com/*"
  ]
}